{
  "name": "dolbyio-web-file-presentation-service",
  "version": "1.0.0",
  "description": "Sample application that uses the File Presentation Service",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "build": "NODE_ENV=development webpack --config webpack.config.js --config webpack.mixer.config.js --progress",
    "prod": "NODE_ENV=production webpack --config webpack.config.js --config webpack.mixer.config.js --progress",
    "start": "node ./server/index.js",
    "watch": "npm-watch build"
  },
  "watch": {
    "build": {
      "patterns": [
        "client"
      ],
      "extensions": "js,jsx,less,html"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolbyio-samples/dolbyio-web-file-presentation-service.git"
  },
  "author": "Fabien Lavocat",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/dolbyio-samples/dolbyio-web-file-presentation-service/issues"
  },
  "homepage": "https://github.com/dolbyio-samples/dolbyio-web-file-presentation-service#readme",
  "dependencies": {
    "@voxeet/voxeet-web-sdk": "^3.1.5",
    "css-loader": "^5.1.3",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "jszip": "^3.6.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "npm-watch": "^0.9.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.22.1",
    "html-webpack-plugin": "^5.3.1",
    "typescript": "^3.8.0",
    "webpack-cli": "^4.5.0"
  }
}
